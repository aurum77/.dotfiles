profile {
  output eDP-1 enable mode 1920x1080@60Hz
}

profile double {
  output eDP-1 enable mode 1920x1080 position 2560,550 scale 1.25
  output HDMI-A-2 mode 2560x1440 position 0,0
}

profile single {
  output HDMI-A-2 mode 2560x1440 position 0,0
  output eDP-1 disable
}

profile share {
  output eDP-1 enable mode 1920x1080 position 1920,0 transform normal scale 1
  output HDMI-A-2 mode 1920x1080 position 0,0
}
