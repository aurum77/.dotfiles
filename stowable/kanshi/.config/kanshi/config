profile {
  output eDP-1 enable
}

profile double {
  output eDP-1 enable mode 1920x1080 position 2560,940 scale 1.25
  output HDMI-A-1 mode 2560x1440 position 0,0
}

profile share {
  output eDP-1 enable mode 1920x1080 position 1920,0 transform normal scale 1
  output HDMI-A-1 mode 1920x1080 position 0,0
}
